{
  "name": "joekolade/visibility",
  "type": "typo3-cms-extension",
  "description": "FSC responsive visibility helper tool for content elements. Make it possible to hide content elements for specific breakpoints.",
  "homepage": "https://github.com/joekolade/visibility",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Joe Sch√§fer",
      "email": "joe@joekola.de",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^10.4.0"
  },
  "autoload": {
    "psr-4": {
      "Joekolade\\Visibility\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Joekolade\\Visibility\\Tests\\": "Tests"
    }
  },
  "replace": {
    "visibility": "self.version",
    "typo3-ter/visibility": "self.version"
  }
}
